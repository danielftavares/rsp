{
  "name": "dcm4chee-arc-ui",
  "private": true,
  "version": "0.0.1",
  "description": "Web Frontend for dcm4chee-arc DICOM Archive",
  "repository": "https://github.com/dcm4che/dcm4chee-arc-light",
  "author": "Gunter Zeilinger <gunterze@gmail.com>",
  "license": "MPL 1.1/GPL 2.0/LGPL 2.1",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "test": "npm run build-js && npm run build-css",
    "build": "npm run build-js && npm run build-css",
    "build-css": "node-sass  src/main/resources/css/rsp.scss  src/main/webapp/css/rsp.css",
    "build-js": "browserify --extension=.jsx --extension=.js src/main/resources/js/main.jsx  > src/main/webapp/js/rsp.js"
  },
  "dependencies": {
    "material-ui": "^0.14.3",
    "react": "^0.14.6",
    "react-dom":"^0.14.6",
    "react-tap-event-plugin": "^0.2.1",
    "react-addons-linked-state-mixin": "^0.14.6",
    "react-router":"1.0.3",
    "history": "1.17.0"
  },
  "devDependencies": {
    "babelify": "^6.1.0",
    "browser-sync": "^2.1.6",
    "browserify": "^8.0.3",
    "clean-css": "^3.1.9",
    "eslint": "^0.14.1",
    "nodemon": "^1.5.0",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "karma": "0.13.15",
    "grunt-karma": "^0.12.1",
    "node-sass": "^3.4.2",
    "reqwest": "^2.0.5"
  }
}